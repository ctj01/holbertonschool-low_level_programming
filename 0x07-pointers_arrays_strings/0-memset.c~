#include "holberton.h"
/**
 *_strcat - check the code for Holberton School students.
 *@dest: is a parameter
 *@src : is a parameter
 * Return: nothing.
 */
char *_strcat(char *dest, char *src)
{
	int i = 0;
	int d = 0;

	while (dest[d] || dest[d] != '\0')
	{

		d++;

	}
	while (src[i] || src[i] != '\0')
	{

		dest[d] = src[i];
		i++;
		d++;

	}

	dest[d] = '\0';

	return (dest);

}
